<header class="navbar navbar-fixed-top navbar-inverse">
   <div class="navbar-inner">
      <div class="container">
        <%= link_to "cabin-parts", root_path, id: "logo" %>
       <nav>
       <form class="navbar-form pull-right form-search">
<div class="input-append">
           <p>
             <%= form_tag ipcboeings_path, :method => 'post' do %>
             <%= text_field_tag :search, params[:search] %>
      
   <%= button_tag '<i class="icon-search"></i>'.html_safe, :class=>"btn", :description => nil %></p>
             <% end %>
           </div>
   
        <ul class="nav pull-right">
          <li><%= link_to "Home", root_path %></li>

        
          <% if signed_in? %>

            <li id="fat-menu" class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                AC Data & Parts<b class="caret"></b>
              </a>
              <ul class="dropdown-menu">
                <li><%= link_to "Airbus", ipc_airbuses_path %></li>
                <li><%= link_to "Boeing", ipcboeings_path %></li>
                <li><%= link_to "SPM List", spm_list_path  %></li>
                <li><%= link_to "Seats", '#' %></li>
                <li><%#= link_to "Tools", '#' %></li>
                <li><%#= link_to "TSM", '#' %></li>
                <li><%#= link_to "Faults", '#' %></li>
                <li><%#= link_to "EO's", '#' %></li>
                <li><%#= link_to "Material", '#' %></li>
              </ul>
            </li>


            <li><%= link_to "Users", users_path %></li>
            <li id="fat-menu" class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                Account <b class="caret"></b>
              </a>
              <ul class="dropdown-menu">
                <li><%= link_to "Profile", current_user %></li>
                <li><%= link_to "Settings", edit_user_path(current_user) %></li>
                <li class="divider"></li>
                <li>
                  <%= link_to "Sign out", signout_path, method: "delete" %>
                </li>
              </ul>
            </li>
          <% else %>
            <li><%#= link_to "Sign in", signin_path %></li>
          <% end %>
        </ul>
      </nav>
    </div>

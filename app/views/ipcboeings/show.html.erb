<%- model_class = Ipcboeing -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ac_type) %>:</strong></dt>
  <dd><%= @ipcboeing.ac_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ata) %>:</strong></dt>
  <dd><%= @ipcboeing.ata %></dd>
  <dt><strong><%= model_class.human_attribute_name(:system) %>:</strong></dt>
  <dd><%= @ipcboeing.system %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @ipcboeing.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:location) %>:</strong></dt>
  <dd><%= @ipcboeing.location %></dd>
  <dt><strong><%= model_class.human_attribute_name(:add_infos) %>:</strong></dt>
  <dd><%= @ipcboeing.add_infos %></dd>
  <dt><strong><%= model_class.human_attribute_name(:part_number) %>:</strong></dt>
  <dd><%= @ipcboeing.part_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:add_material_info) %>:</strong></dt>
  <dd><%= @ipcboeing.add_material_info %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ipc) %>:</strong></dt>
  <dd><%= @ipcboeing.ipc %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              ipcboeings_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ipcboeing_path(@ipcboeing), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ipcboeing_path(@ipcboeing),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

<%- model_class = Ipcboeing -%>
<div class="page-header">
  <!-- <h1><%#=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1> -->
  <h1>Boeing Parts List</h1>
</div>
<div class="tab-content">
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= sortable model_class.human_attribute_name(:id) %></th>
      <th><%= sortable model_class.human_attribute_name(:ac_type) %></th>
      <th><%= sortable model_class.human_attribute_name(:ata) %></th> 
      <th><%= sortable model_class.human_attribute_name(:system) %></th>
      <th><%= sortable model_class.human_attribute_name(:description) %></th>
      <th><%= sortable model_class.human_attribute_name(:location) %></th>
      <th><%= sortable model_class.human_attribute_name(:add_infos) %></th>
      <th><%= sortable model_class.human_attribute_name(:part_number) %></th>
      <th><%= sortable model_class.human_attribute_name(:add_material_info) %></th>
      <th><%= sortable model_class.human_attribute_name(:ipc) %></th>
      <th><%#= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @ipcboeings.each do |ipcboeing| %>
      <tr>
        <td><%= link_to ipcboeing.id, ipcboeing_path(ipcboeing) %></td>
        <td><%= ipcboeing.ac_type %></td>
        <td><%= ipcboeing.ata %></td>
        <td><%= ipcboeing.system %></td>
        <td><%= ipcboeing.description %></td>
        <td><%= ipcboeing.location %></td>
        <td><%= ipcboeing.add_infos %></td>
        <td><%= ipcboeing.part_number %></td>
        <td><%= ipcboeing.add_material_info %></td>
        <td><%= ipcboeing.ipc %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_ipcboeing_path(ipcboeing), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      ipcboeing_path(ipcboeing),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>

   <%= link_to t('.new', :default => t("helpers.links.new")),
               new_ipcboeing_path,
               :class => 'btn btn-primary' %>
     <div>
     </br>
   <footer class="footer">
      <nav>
           <h3>Import Boeing-File</h3>
           <%= form_tag import_ipcboeings_path, multipart: true do %>
           <%= file_field_tag :file, :class => 'btn btn-mini' %>
           <%= submit_tag "Import", :class => 'btn btn-primary' %>
      <% end %>
   </div>
  </nav>
</footer>
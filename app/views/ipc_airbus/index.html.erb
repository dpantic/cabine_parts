<%- model_class = IpcAirbus -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:ac_type) %></th>
      <th><%= model_class.human_attribute_name(:ata) %></th>
      <th><%= model_class.human_attribute_name(:system) %></th>
      <th><%= model_class.human_attribute_name(:description) %></th>
      <th><%= model_class.human_attribute_name(:fin) %></th>
      <th><%= model_class.human_attribute_name(:add_infos) %></th>
      <th><%= model_class.human_attribute_name(:part_number) %></th>
      <th><%= model_class.human_attribute_name(:add_material_info) %></th>
      <th><%= model_class.human_attribute_name(:ipc) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @ipc_airbuses.each do |ipc_airbus| %>
      <tr>
        <td><%= link_to ipc_airbus.id, ipc_airbus_path(ipc_airbus) %></td>
        <td><%= ipc_airbus.ac_type %></td>
        <td><%= ipc_airbus.ata %></td>
        <td><%= ipc_airbus.system %></td>
        <td><%= ipc_airbus.description %></td>
        <td><%= ipc_airbus.fin %></td>
        <td><%= ipc_airbus.add_infos %></td>
        <td><%= ipc_airbus.part_number %></td>
        <td><%= ipc_airbus.add_material_info %></td>
        <td><%= ipc_airbus.ipc %></td>
        <td><%=l ipc_airbus.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_ipc_airbus_path(ipc_airbus), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      ipc_airbus_path(ipc_airbus),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_ipc_airbus_path,
            :class => 'btn btn-primary' %>

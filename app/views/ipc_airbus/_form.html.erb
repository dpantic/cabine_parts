<%= form_for @ipc_airbus, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :ac_type, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :ac_type, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :ata, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :ata, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :system, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :system, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :description, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :description, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :fin, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :fin, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :add_infos, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :add_infos, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :part_number, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :part_number, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :add_material_info, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :add_material_info, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :ipc, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :ipc, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                ipc_airbus_path, :class => 'btn' %>
  </div>
<% end %>

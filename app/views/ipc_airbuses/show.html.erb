f<%- model_class = IpcAirbus -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ac_type) %>:</strong></dt>
  <dd><%= @ipc_airbus.ac_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ata) %>:</strong></dt>
  <dd><%= @ipc_airbus.ata %></dd>
  <dt><strong><%= model_class.human_attribute_name(:system) %>:</strong></dt>
  <dd><%= @ipc_airbus.system %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @ipc_airbus.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fin) %>:</strong></dt>
  <dd><%= @ipc_airbus.fin %></dd>
  <dt><strong><%= model_class.human_attribute_name(:add_infos) %>:</strong></dt>
  <dd><%= @ipc_airbus.add_infos %></dd>
  <dt><strong><%= model_class.human_attribute_name(:part_number) %>:</strong></dt>
  <dd><%= @ipc_airbus.part_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:add_material_info) %>:</strong></dt>
  <dd><%= @ipc_airbus.add_material_info %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ipc) %>:</strong></dt>
  <dd><%= @ipc_airbus.ipc %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              ipc_airbuses_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ipc_airbus_path(@ipc_airbus), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ipc_airbus_path(@ipc_airbus),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
